<Window x:Class="StudioAdvanced.Views.winPrintSchedule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:RV="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        xmlns:local="clr-namespace:StudioAdvanced.Views"
        mc:Ignorable="d"
        Title="Print Student Schedule" WindowState="Maximized" Loaded="Window_Loaded">
    <Grid>
        <WindowsFormsHost>
            <RV:ReportViewer x:Name="ReportStudentSchedule"></RV:ReportViewer>
        </WindowsFormsHost>
    </Grid>
</Window>
